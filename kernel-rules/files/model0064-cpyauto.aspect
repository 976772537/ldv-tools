before: file ("$this")
{
#include <linux/kernel.h>
extern char *ldv_env_strncat(char *dest, const char *src, size_t count, size_t size_of_dest);
}


before : call(char * strncat(char *, const char *, __kernel_size_t))
{
/*
	$arg_size1 - размер массива вычисленный в точке вызова
	или -1 если не удалось вычислить
*/
	if($arg_size1!=-1) {
		ldv_env_strncat($arg1,$arg2,$arg3,$arg_size1);
	}
}
